{
  "$schema": "https://mintlify.com/schema.json",
  "theme": "maple",
  "name": "Buildstash",
  "logo": {
    "dark": "/logo/dark.svg",
    "light": "/logo/light.svg"
  },
  "favicon": "/favicon.svg",
  "colors": {
    "primary": "#9999ff",
    "light": "#b3b3ff",
    "dark": "#6666ff"
  },
  "api": {
    "mdx": {
      "server": "https://app.buildstash.com/api/v1",
      "auth": {
        "method": "key",
        "name": "bearer"
      }
    },
    "playground": {
      "display": "simple"
    },
   "examples": {
     "defaults": "required"
   }
  },
  "navigation": {
    "languages": [
      {
        "language": "en",
        "groups": [
          {
            "group": "Getting started",
            "pages": [
              "introduction"
            ]
          },
          {
            "group": "Upload",
            "pages": [
              "upload"
            ]
          }
        ]
      }
    ]
  },
  "global": {
    "anchors": []
  },
  "redirects": [
    {
      "source": "/changelog",
      "destination": "https://support.buildstash.com/changelog"
    },
    {
      "source": "/changelog/overview",
      "destination": "https://support.buildstash.com/changelog"
    },
    {
      "source": "/integrations",
      "destination": "https://support.buildstash.com/docs/integrations/overview"
    },
    {
      "source": "/integrations/ci/github",
      "destination": "https://support.buildstash.com/docs/integrations/github-actions"
    },
    {
      "source": "/integrations/sso/jumpcloud",
      "destination": "https://support.buildstash.com/docs/integrations/jumpcloud"
    },
    {
      "source": "/guide",
      "destination": "https://support.buildstash.com/docs"
    }
  ],
  "footerSocials": {
    "x": "https://x.com/buildstash",
    "github": "https://github.com/buildstash",
    "linkedin": "https://linkedin.com/company/buildstash"
  }
}